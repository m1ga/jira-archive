					"author": {
						"name": "iblack.dum",
						"key": "iblack.dum",
						"displayName": "Sathaphorn Kerdmalai",
						"active": true,
						"timeZone": "Asia/Bangkok"
					},
					"body": "Here is manifest file.\r\n[^manifest]\r\nand i used  ./build.py in each project to build module.",
					"updateAuthor": {
						"name": "iblack.dum",
						"key": "iblack.dum",
						"displayName": "Sathaphorn Kerdmalai",
						"active": true,
						"timeZone": "Asia/Bangkok"
					},
