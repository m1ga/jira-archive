				{
					"id": "307334",
					"author": {
						"name": "skypanther",
						"key": "skypanther",
						"displayName": "Tim Poulsen",
						"active": true,
						"timeZone": "America/New_York"
					},
					"body": "The \\ue012 character appears to not be supported on Android. Try the test case using \\u2764 (a heart) instead. It works fine for me.\r\n\r\nhttp://www.fileformat.info/info/unicode/char/e012/fontsupport.htm doesn't list Droid Sans or Roboto as supporting the e012 character\r\n\r\nPerhaps installing this app will help you determine which fonts are supported on your android devices: http://www.madore.org/~david/programs/UnicodeMap.html",
					"updateAuthor": {
						"name": "skypanther",
						"key": "skypanther",
						"displayName": "Tim Poulsen",
						"active": true,
						"timeZone": "America/New_York"
					},
					"created": "2014-06-03T15:33:48.000+0000",
					"updated": "2014-06-03T15:33:48.000+0000"
				},
