					},
					"body": "{html}<div><p>Same as <a href=\n\"https://appcelerator.lighthouseapp.com/projects/32238-titanium-mobile/tickets/302-add-support-for-uipastboard#ticket-302-2\">\nhttps://appcelerator.lighthouseapp.com/projects/32238-titanium-mobi...</a>\n?</p></div>{html}",
					"updateAuthor": {
						"name": "brionvibber",
						"key": "brionvibber",
						"displayName": "Brion Vibber",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
