						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "{html}<div><p>I would also LOVE, LOVE to see this added to Titanium Mobile</p></div>{html}",
					"updateAuthor": {
						"name": "kevin(wa)",
						"key": "kevin(wa)",
						"displayName": "Kevin (WA)",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
