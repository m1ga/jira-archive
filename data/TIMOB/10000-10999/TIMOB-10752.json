					"body": "Titanium SDK master pull request: https://github.com/appcelerator/titanium_mobile/pull/5621\nTitanium CLI master pull request: https://github.com/appcelerator/titanium/pull/127",
					"updateAuthor": {
						"name": "cbarber",
						"key": "cbarber",
						"displayName": "Chris Barber",
