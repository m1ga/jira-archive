				{
					"id": "377778",
					"author": {
						"name": "bgrantges@appcelerator.com",
						"key": "bgrantges",
						"displayName": "Bert Grantges",
						"active": false,
						"timeZone": "America/Chicago"
					},
					"body": "As we look to add this API I would like to ensure that we look at it from a parity perspective with the inclusion of Android and Windows GridView classes as we can leverage Hyperloop now for one offs",
					"updateAuthor": {
						"name": "bgrantges@appcelerator.com",
						"key": "bgrantges",
						"displayName": "Bert Grantges",
						"active": false,
