		"environment": null,
		"comment": {
			"comments": [
				{
					"id": "255470",
					"author": {
						"name": "tlukasavage",
						"key": "tlukasavage",
						"displayName": "Tony Lukasavage",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "As noted in the related ticket, this should likely be a different name to avoid confusion with Ti.UI.TableViewRow.classname. I made a few suggestions, but I like *apiName*.",
					"updateAuthor": {
						"name": "tlukasavage",
						"key": "tlukasavage",
						"displayName": "Tony Lukasavage",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"created": "2013-06-03T12:51:55.000+0000",
					"updated": "2013-06-03T12:51:55.000+0000"
				},
				{
					"id": "257713",
					"author": {
						"name": "bhughes",
						"key": "bhughes",
						"displayName": "Bryan Hughes",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "The architecture council approved the following spec:\r\n\r\n{code}\r\n  - name: apiName\r\n    summary: The name of the API that this proxy corresponds to.\r\n    description: |\r\n        Each proxy will expose the name of the API that corresponds to this proxy. The value\r\n        of this constant is the fully qualified name of the API. For example, a [Button](Titanium.UI.Button)\r\n        will return \"Ti.UI.Button\" for apiName.\r\n    permission: read-only\r\n    type: String\r\n    since: \"3.1.2\"\r\n{code}",
					"updateAuthor": {
						"name": "bhughes",
						"key": "bhughes",
						"displayName": "Bryan Hughes",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"created": "2013-06-13T19:05:55.000+0000",
					"updated": "2013-06-13T19:06:27.000+0000"
				}
			],
