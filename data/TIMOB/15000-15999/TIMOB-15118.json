					"body": "Backport PR: https://github.com/appcelerator/titanium_mobile/pull/5436",
					"updateAuthor": {
						"name": "penrique",
						"key": "penrique",
						"displayName": "Pedro Enrique",
						"active": false,
						"timeZone": "America/Los_Angeles"
