					"author": {
						"name": "jason919",
						"key": "jason919",
						"displayName": "jason hu",
						"active": true,
						"timeZone": "America/Chicago"
					},
					"body": "yes, it can always detect all my devices. I tried suggested dab kill-server/start-server  as well",
					"updateAuthor": {
						"name": "jason919",
						"key": "jason919",
						"displayName": "jason hu",
						"active": true,
						"timeZone": "America/Chicago"
					},
					"created": "2014-01-08T20:39:24.000+0000",
