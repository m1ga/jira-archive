				{
					"id": "394614",
					"author": {
						"name": "cng",
						"key": "cng",
						"displayName": "Chee Kiat Ng",
						"active": false,
						"timeZone": "America/Los_Angeles"
					},
					"body": "It looks like the Ti.userAgent for Android is read only, as it is the equivalent of the Android System default user agent. \r\nThe only useragent that you wish to set would be in webView, or the request header of HttpClient.",
					"updateAuthor": {
						"name": "cng",
						"key": "cng",
						"displayName": "Chee Kiat Ng",
						"active": false,
						"timeZone": "America/Los_Angeles"
					},
