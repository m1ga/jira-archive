						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "Reopening to investigate crash found by Thom.",
					"updateAuthor": {
						"name": "stephentramer",
						"key": "stephentramer",
						"displayName": "Stephen Tramer",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
