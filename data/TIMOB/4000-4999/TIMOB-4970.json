					"body": "The default project still comes without a default iTunesArtwork file when creating a new project via Titanium Studio.  ",
					"updateAuthor": {
						"name": "jyounus",
						"key": "jyounus",
						"displayName": "Junaid Younus",
						"active": true,
						"timeZone": "Europe/London"
					},
