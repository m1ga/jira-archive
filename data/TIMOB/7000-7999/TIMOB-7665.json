					"body": "We have Gzip compression enabled in both sending and receiving in all comms by default. Moving to mark this invalid.",
					"updateAuthor": {
						"name": "blainhamon",
						"key": "blainhamon",
						"displayName": "Blain Hamon",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
