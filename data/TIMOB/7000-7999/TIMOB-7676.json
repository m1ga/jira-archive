					"body": "Sent [PR #1455|https://github.com/appcelerator/titanium_mobile/pull/1455] to resolve issue.",
					"updateAuthor": {
						"name": "joshroesslein",
						"key": "joshroesslein",
						"displayName": "Josh Roesslein",
						"active": true,
						"timeZone": "America/Los_Angeles"
