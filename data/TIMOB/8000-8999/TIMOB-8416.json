			"comments": [
				{
					"id": "189174",
					"author": {
						"name": "dhyde",
						"key": "dhyde",
						"displayName": "Dustin Hyde",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "Also occurs on Emulator 3.0. Screenshot attached.",
					"updateAuthor": {
						"name": "dhyde",
						"key": "dhyde",
						"displayName": "Dustin Hyde",
						"active": true,
						"timeZone": "America/Los_Angeles"
