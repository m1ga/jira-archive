				{
					"id": "198449",
					"author": {
						"name": "dlifshitz",
						"key": "dlifshitz",
						"displayName": "David Lifshitz",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"body": "A quick look at KS shows the following needs to be implemented:\r\n* Titanium.UI.createTabGroup\r\n* Titanium.UI.createTab\r\n* Titanium.UI.createWindow\r\n** url\r\n* Titanium.UI.createView\r\n* Ti.Platform.osname\r\n* Ti.include\r\n* Ti.API.info\r\n* Ti.App.Properties\r\n* Ti.UI.createAlertDialog\r\n* Ti.Platform.openURL",
					"updateAuthor": {
						"name": "dlifshitz",
						"key": "dlifshitz",
						"displayName": "David Lifshitz",
						"active": true,
						"timeZone": "America/Los_Angeles"
					},
					"created": "2012-06-14T06:20:55.000+0000",
					"updated": "2012-06-14T07:24:46.000+0000"
				},
